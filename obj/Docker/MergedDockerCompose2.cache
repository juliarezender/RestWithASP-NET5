networks:
  udemy-network:
    driver: bridge
services:
  db:
    build:
      context: C:\Users\julia\Desktop\RestWithASP-NET5
      dockerfile: db/Dockerfile
    command: mysqld --default-authentication-plugin=mysql_native_password
    environment:
      MYSQL_DATABASE: rest_with_asp_net_udemy
      MYSQL_PASSWORD: docker
      MYSQL_ROOT_PASSWORD: docker
      MYSQL_USER: docker
      TZ: America/Sao_Paulo
    image: rest-with-asp-net-udemy-db
    networks:
      udemy-network: {}
    ports:
    - published: 3308
      target: 3306
    restart: always
  rest-with-asp-net-udemy:
    build:
      context: C:\Users\julia\Desktop\RestWithASP-NET5
      dockerfile: RestWithASPNETUdemy/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: RestWithASPNETUdemy
      target: base
    container_name: RestWithASPNETUdemy
    depends_on:
      db:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Production
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
      TZ: America/Sao_Paulo
    image: rest-with-asp-net-udemy:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/RestWithASPNETUdemy.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      udemy-network: {}
    ports:
    - published: 44300
      target: 80
    tty: true
    volumes:
    - C:\Users\julia\Desktop\RestWithASP-NET5\RestWithASPNETUdemy:/app:rw
    - C:\Users\julia\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\julia\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\julia\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\julia\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\julia\Desktop\RestWithASP-NET5:/src:rw
version: '3.4'